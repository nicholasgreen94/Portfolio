{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:3001",
      "secure": false
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "helmet": "^3.12.1",
    "history": "^4.7.2",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-masonry-component": "^6.2.1",
    "react-router": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/assets/css --include-path ./node_modules src/assets/css -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/assets/css --include-path ./node_modules src/assets/css -o src/assets/css --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
